{
  "name": "react-vite-biome-tailwind-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "biome:ci": "biome ci ./src",
